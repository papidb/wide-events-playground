{
  "name": "my-app",
  "type": "module",
  "scripts": {
    "start": "pnpm build && NODE_ENV=production tsx src/index.tsx",
    "dev": "concurrently \"npm run dev-hono\" \"npm run dev-vite\" ",
    "dev-hono": "tsx watch src/index.tsx",
    "dev-vite": "vite",
    "build": "vite build",
    "generate-data": "node generate-data.js 100000               ",
    "serve": "NODE_ENV=production pnpm run src/index.tsx"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@hono/node-server": "^1.13.7",
    "concurrently": "^9.1.0",
    "duckdb": "^1.1.3",
    "duckdb-async": "^1.1.3",
    "hono": "^4.6.10"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.7.1",
    "vite": "^5.4.11"
  }
}
